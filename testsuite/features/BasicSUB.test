#
# Yoshimi-Testsuite: base Alogorithms
#
description = SUBSynth tone with bandwidth

[Test]
Script
    set part 1
    set add off
    ..
    set sub on
    set band width 80
    set band scale 10
    /
    set test note 42
    set scalestep 6
    set repetitions 6
    set duration 0.5
    set holdfraction 0.9
    execute
End-Script

verifySound = On
verifyTimes = Off

#
# Sample calculation time
#
description = Runtime: PAD-Synth / 1 note / 0.1s

[Test]
Script
    set part 1 ADD off
    ..
    set PAD on
    set bandwidth 711
    set waveform ABSsine
    set APPLY FORCE
    /
    set test
    set duration 0.1
    set holdfraction 1.0
    set repetitions 1
    execute
End-Script

verifySound = Off
verifyTimes = On

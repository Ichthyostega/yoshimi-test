#
# Sample calculation time
#
description = Runtime: PAD-Synth / 1 note / 1s

[Test]
Script
    set config buildPAD Muted
    # build PADSynth wavetable single threaded
    # (avoid concurrency overhead for timings)
    /
    set part 1 ADD off
    ..
    set PAD on
    set bandwidth 711
    set waveform ABSsine
    /
    set test
    set duration 1
    set holdfraction 1.0
    set repetitions 1
    execute
End-Script

verifySound = Off
verifyTimes = On

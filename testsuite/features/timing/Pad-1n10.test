#
# Impact of NoteON events
#
description = Runtime: PAD-Synth / 10 notes / overall 1.0133s (48640 samples)

[Test]
Script
    set config buildPAD Muted
    # build PADSynth wavetable single threaded
    # (avoid concurrency overhead for timings)
    /
    set part 1 ADD off
    ..
    set PAD on
    set bandwidth 711
    set waveform ABSsine
    ..
    set filter type h2
    set center 10
    set Q 1
    /
    set test
    set note 36
    set duration 0.1
    set holdfraction 0.9
    set repetitions 10
    execute
End-Script

verifySound = Off
verifyTimes = On

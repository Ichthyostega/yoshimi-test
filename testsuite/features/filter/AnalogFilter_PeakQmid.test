#
# Yoshimi-Testsuite: AnalogFilter
#
description = Filter response Peak (2pole) middle frequency default Q (40)

[Test]
Script
    set part 1 ADD
    set filter
    set type Peak
    set center 64
    set gain 120
    /
    set test note 0
    set scalestep 6
    set repetitions 21
    set duration 0.05
    set holdfraction 0.5
    execute
End-Script

verifySound = On
verifyTimes = Off

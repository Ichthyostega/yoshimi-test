#
# Yoshimi-Testsuite: Oscil Wavetable Generator
#
# Note: bypass all filters
description = Wavetable simple Circle + Func-Par -16  (square with circle segment on top)

[Test]
Script
    set part 1 ADD
    set volume 120
    set voice 1
    set bypass on
    set waveform Circle
    set base par -16
    /
    set test note 32
    set duration 0.04
    set holdfraction 0.75
    set scalestep 18
    execute
End-Script

verifySound = On
verifyTimes = Off

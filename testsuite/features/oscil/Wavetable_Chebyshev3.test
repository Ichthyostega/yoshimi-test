#
# Yoshimi-Testsuite: Oscil Wavetable Generator
#
# Note: bypass all filters
description = Wavetable Chebyshev + Func-Par +63 (squished, dense)

[Test]
Script
    set part 1 ADD
    set volume 120
    set voice 1
    set bypass on
    set waveform Chebyshev
    set base par +63
    /
    set test note 32
    set duration 0.04
    set holdfraction 0.75
    set scalestep 18
    execute
End-Script

verifySound = On
verifyTimes = Off
